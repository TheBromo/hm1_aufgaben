   "source": [
    "import numpy as np\n",
    "\n",
    "A = np.array([[2, 1, 0], [4, 0, -2], [1, 2, 4]])\n",
    "b = np.array([1, 1, 0])\n",
    "\n",
    "detA = np.linalg.det(A)\n",
    "print(detA)\n",
    "\n",
    "xstar = np.linalg.solve(A,b)\n",
    "\n",
    "print(xstar)"
   ]
